Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"g", "[", "t_", "]"}], ":=", 
   RowBox[{"Round", "[", 
    RowBox[{"50", "*", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"4", 
        RowBox[{"Sin", "[", 
         RowBox[{"4", "Pi", "*", "t"}], "]"}]}], "-", 
       RowBox[{"Sign", "[", 
        RowBox[{"t", "-", ".3"}], "]"}], "-", 
       RowBox[{"Sign", "[", 
        RowBox[{".72", "-", "t"}], "]"}]}], ")"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"sigma", ",", "n"}], "}"}], "=", 
   RowBox[{"{", 
    RowBox[{"6", ",", "1024"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"original", "=", 
   RowBox[{"g", "[", 
    RowBox[{"Range", "[", 
     RowBox[{"0", ",", 
      RowBox[{"1", "-", 
       RowBox[{"1", "/", "n"}]}], ",", 
      RowBox[{"1", "/", "n"}]}], "]"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ListPlot", "[", 
   RowBox[{"original", ",", 
    RowBox[{"Ticks", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Range", "[", 
        RowBox[{"0", ",", "n", ",", 
         RowBox[{"n", "/", "4"}]}], "]"}], ",", 
       RowBox[{"Range", "[", 
        RowBox[{
         RowBox[{"-", "4"}], ",", "4", ",", "2"}], "]"}]}], "}"}]}], ",", 
    RowBox[{"PlotLabel", "\[Rule]", "\"\<Original Signal\>\""}]}], "]"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"y", "=", 
   RowBox[{"original", "+", 
    RowBox[{"WhiteNoise", "[", 
     RowBox[{"n", ",", "sigma"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ListPlot", "[", 
   RowBox[{"y", ",", 
    RowBox[{"Ticks", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Range", "[", 
        RowBox[{"0", ",", "n", ",", 
         RowBox[{"n", "/", "4"}]}], "]"}], ",", 
       RowBox[{"Range", "[", 
        RowBox[{
         RowBox[{"-", "4"}], ",", "4", ",", "2"}], "]"}]}], "}"}]}], ",", 
    RowBox[{"PlotLabel", "\[Rule]", "\"\<Noisy Signal\>\""}]}], "]"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"newy", "=", 
   RowBox[{"VisuShrink", "[", 
    RowBox[{"y", ",", 
     RowBox[{"SplineFilters", "[", 
      RowBox[{"2", ",", "2"}], "]"}], ",", 
     RowBox[{"NumIterations", "\[Rule]", "3"}], ",", 
     RowBox[{"Boundary", "\[Rule]", "True"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"ListPlot", "[", 
  RowBox[{"newy", ",", 
   RowBox[{"Ticks", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Range", "[", 
       RowBox[{"0", ",", "n", ",", 
        RowBox[{"n", "/", "4"}]}], "]"}], ",", 
      RowBox[{"Range", "[", 
       RowBox[{
        RowBox[{"-", "4"}], ",", "4", ",", "2"}], "]"}]}], "}"}]}], ",", 
   RowBox[{"PlotLabel", "\[Rule]", "\"\<Denoised Signal\>\""}]}], 
  "]"}]}], "Input",
