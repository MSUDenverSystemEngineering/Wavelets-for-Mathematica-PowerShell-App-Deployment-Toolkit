 Cell["(1)", "ExampleCount"]
}], "ExampleSubsection", "ExampleSubsection",
 WholeCellGroupOpener->True],

Cell[TextData[{
 "The LeGall wavelet transform employs the lifting scheme to perfomr the \
computations and can, by setting ",
 Cell[BoxData[
  TemplateBox[{Cell[
     TextData["IntegerMap"]],"paclet:WaveletWare/ref/IntegerMap"},
   "RefLink",
   BaseStyle->"InlineFormula"]], "InlineFormula"],
 " to ",
 ButtonBox["True",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/True"],
 ", map integer-valued input to integer-valued output.  This option can be \
set in ",
 StyleBox["VISUShrink",
  FontFamily->"Courier New",
  FontSize->12],
 " although we must ensure that the data and noise are integer-valued."
}], "ExampleText",
 CellID->195850320],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"g", "[", "t_", "]"}], ":=", 
   RowBox[{"Round", "[", 
    RowBox[{"50", "*", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"4", 
        RowBox[{"Sin", "[", 
         RowBox[{"4", "Pi", "*", "t"}], "]"}]}], "-", 
       RowBox[{"Sign", "[", 
        RowBox[{"t", "-", ".3"}], "]"}], "-", 
       RowBox[{"Sign", "[", 
        RowBox[{".72", "-", "t"}], "]"}]}], ")"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"sigma", ",", "n"}], "}"}], "=", 
   RowBox[{"{", 
    RowBox[{"6", ",", "1024"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"original", "=", 
   RowBox[{"g", "[", 
    RowBox[{"Range", "[", 
     RowBox[{"0", ",", 
      RowBox[{"1", "-", 
       RowBox[{"1", "/", "n"}]}], ",", 
      RowBox[{"1", "/", "n"}]}], "]"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ListPlot", "[", 
   RowBox[{"original", ",", 
    RowBox[{"Ticks", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Range", "[", 
        RowBox[{"0", ",", "n", ",", 
         RowBox[{"n", "/", "4"}]}], "]"}], ",", 
       RowBox[{"Range", "[", 
        RowBox[{
         RowBox[{"-", "4"}], ",", "4", ",", "2"}], "]"}]}], "}"}]}], ",", 
    RowBox[{"PlotLabel", "\[Rule]", "\"\<Original Signal\>\""}]}], "]"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"y", "=", 
   RowBox[{"original", "+", 
    RowBox[{"Round", "[", 
     RowBox[{"WhiteNoise", "[", 
      RowBox[{"n", ",", "sigma"}], "]"}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ListPlot", "[", 
   RowBox[{"y", ",", 
    RowBox[{"Ticks", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Range", "[", 
        RowBox[{"0", ",", "n", ",", 
         RowBox[{"n", "/", "4"}]}], "]"}], ",", 
       RowBox[{"Range", "[", 
        RowBox[{
         RowBox[{"-", "4"}], ",", "4", ",", "2"}], "]"}]}], "}"}]}], ",", 
    RowBox[{"PlotLabel", "\[Rule]", "\"\<Noisy Signal\>\""}]}], "]"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"newy", "=", 
   RowBox[{"VisuShrink", "[", 
    RowBox[{"y", ",", 
     RowBox[{"LeGall", "[", "]"}], ",", 
     RowBox[{"NumIterations", "\[Rule]", "3"}], ",", 
     RowBox[{"IntegerMap", "\[Rule]", "True"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"ListPlot", "[", 
  RowBox[{"newy", ",", 
   RowBox[{"Ticks", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Range", "[", 
       RowBox[{"0", ",", "n", ",", 
        RowBox[{"n", "/", "4"}]}], "]"}], ",", 
      RowBox[{"Range", "[", 
       RowBox[{
        RowBox[{"-", "4"}], ",", "4", ",", "2"}], "]"}]}], "}"}]}], ",", 
   RowBox[{"PlotLabel", "\[Rule]", "\"\<Denoised Signal\>\""}]}], 
  "]"}]}], "Input",
 CellLabel->"In[1]:=",
 CellID->297055439],

