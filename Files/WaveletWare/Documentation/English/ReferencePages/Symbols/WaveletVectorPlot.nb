Cell[TextData[{
 "By default, ",
 StyleBox["WaveletVectorPlot",
  FontFamily->"Courier New",
  FontSize->12],
 " plots all iterations of the wavelet transform, but the option Iteration \
can be used to highlight particular iteration.  In this case, ",
 Cell[BoxData[
  TemplateBox[{Cell[
     TextData["Iteration"]],"paclet:WaveletWare/ref/Iteration"},
   "RefLink",
   BaseStyle->"InlineFormula"]], "InlineFormula"],
 " should be given as a list of ordered pairs ",
 Cell[BoxData[
  RowBox[{"(", 
   RowBox[{
    StyleBox["i", "TI"], ",", 
    StyleBox["j", "TI"]}], ")"}]], "InlineFormula"],
 " where ",
 Cell[BoxData[
  StyleBox["i", "TI"]], "InlineFormula"],
 " is the iteration and ",
 Cell[BoxData[
  StyleBox["j", "TI"]], "InlineFormula"],
 " is the portion of the ",
 Cell[BoxData[
  StyleBox["i", "TI"]], "InlineFormula"],
 "th iteration.  Note that only in the case when ",
 Cell[BoxData[
  StyleBox["i", "TI"]], "InlineFormula"],
 " equals ",
 Cell[BoxData[
  TemplateBox[{Cell[
     TextData["NumIterations"]],"paclet:WaveletWare/ref/NumIterations"},
   "RefLink",
   BaseStyle->"InlineFormula"]], "InlineFormula"],
 ", ",
 Cell[BoxData[
  StyleBox["j", "TI"]], "InlineFormula"],
 "=1,2.   For ",
 Cell[BoxData[
  StyleBox["i", "TI"]], "InlineFormula"],
 "<",
 Cell[BoxData[
  TemplateBox[{Cell[
     TextData["NumIterations"]],"paclet:WaveletWare/ref/NumIterations"},
   "RefLink",
   BaseStyle->"InlineFormula"]], "InlineFormula"],
 ", ",
 Cell[BoxData[
  StyleBox["j", "TI"]], "InlineFormula"],
 "=2."
}], "ExampleText",
 CellID->68303572],

Cell[TextData[{
 "Regions not identified by ",
 Cell[BoxData[
  TemplateBox[{Cell[
     TextData["Iteration"]],"paclet:WaveletWare/ref/Iteration"},
   "RefLink",
   BaseStyle->"InlineFormula"]], "InlineFormula"],
 " are plotted in a light gray color.  The grav scale value of the regions \
not selected can be controlled by the ",
 Cell[BoxData[
  TemplateBox[{Cell[
     TextData["Dimming"]],"paclet:WaveletWare/ref/Dimming"},
   "RefLink",
   BaseStyle->"InlineFormula"]], "InlineFormula"],
 " option."
}], "ExampleText",
 CellID->411750519],

