  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"Donoho", "/", "Johnstone"}], " ", "Doppler", " ", "function"}], 
   " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"g", "[", "t_", "]"}], ":=", 
     RowBox[{
      RowBox[{"Sqrt", "[", 
       RowBox[{"t", 
        RowBox[{"(", 
