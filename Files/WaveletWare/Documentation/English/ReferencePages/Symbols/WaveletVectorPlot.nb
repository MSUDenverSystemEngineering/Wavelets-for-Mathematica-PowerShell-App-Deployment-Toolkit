  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"Donoho", "/", "Johnstone"}], " ", "Doppler", " ", "function"}], 
   " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"g", "[", "t_", "]"}], ":=", 
     RowBox[{
      RowBox[{"Sqrt", "[", 
       RowBox[{"t", 
        RowBox[{"(", 
         RowBox[{"1", "-", "t"}], ")"}]}], "]"}], "*", 
      RowBox[{"Sin", "[", 
       RowBox[{"2", "Pi", 
        RowBox[{
         RowBox[{"(", "1.05", ")"}], "/", 
         RowBox[{"(", 
          RowBox[{"t", "+", ".05"}], ")"}]}]}], "]"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"n", "=", "1024"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"v", "=", 
     RowBox[{"g", "[", 
      RowBox[{"Range", "[", 
       RowBox[{"0", ",", 
        RowBox[{"1", "-", 
         RowBox[{"1", "/", "n"}]}], ",", 
        RowBox[{"1", "/", "n"}]}], "]"}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"ListPlot", "[", 
    RowBox[{"v", ",", 
     RowBox[{"Ticks", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Range", "[", 
         RowBox[{"0", ",", "n", ",", 
          RowBox[{"n", "/", "4"}]}], "]"}], ",", 
        RowBox[{"Range", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"-", "1"}], "/", "2"}], ",", 
          RowBox[{"1", "/", "2"}]}], "]"}]}], "}"}]}], ",", 
     RowBox[{"PlotLabel", "\[Rule]", "\"\<Data Samples\>\""}]}], "]"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{"wpt", ",", "tree"}], "}"}], "=", 
     RowBox[{"WPT", "[", 
      RowBox[{"v", ",", 
       RowBox[{"Daub", "[", "4", "]"}], ",", 
       RowBox[{"NumIterations", "\[Rule]", "3"}], ",", 
       RowBox[{"CostFunction", "\[Rule]", "NumberAboveThreshold"}], ",", 
       RowBox[{"SecondParameter", "\[Rule]", ".1"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
