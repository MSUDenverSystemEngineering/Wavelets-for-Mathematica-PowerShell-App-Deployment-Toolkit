Cell[TextData[{
 StyleBox["WaveletVectorPlot",
  FontFamily->"Courier New",
  FontSize->12],
 " plots the different regions of the wavelet transform using the default \
colors given by the ",
 Cell[BoxData[
  TemplateBox[{Cell[
     TextData["Colors"]],"paclet:WaveletWare/ref/Colors"},
   "RefLink",
   BaseStyle->"InlineFormula"]], "InlineFormula"],
 " options.  The length of the ",
 Cell[BoxData[
  TemplateBox[{Cell[
     TextData["Colors"]],"paclet:WaveletWare/ref/Colors"},
   "RefLink",
   BaseStyle->"InlineFormula"]], "InlineFormula"],
 " list is 30.  If the transform consists of more than 30 regions, extra \
colors from the beginning of the list are added.  The user can control the \
colors of each plotted region.  "
}], "ExampleText",
 CellID->101982555],

Cell[TextData[{
 "Note that the length of ",
 Cell[BoxData[
  TemplateBox[{Cell[
     TextData["Colors"]],"paclet:WaveletWare/ref/Colors"},
   "RefLink",
   BaseStyle->"InlineFormula"]], "InlineFormula"],
 " needs to be at least as long as ",
 Cell[BoxData[
  TemplateBox[{Cell[
     TextData["NumIterations"]],"paclet:WaveletWare/ref/NumIterations"},
   "RefLink",
   BaseStyle->"InlineFormula"]], "InlineFormula"],
 " + 1."
}], "ExampleText",
 CellID->91816100],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"Donoho", " ", "Heaviside", " ", "function"}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"g", "[", "t_", "]"}], ":=", 
     RowBox[{
      RowBox[{"4", 
       RowBox[{"Sin", "[", 
        RowBox[{"4", "Pi", "*", "t"}], "]"}]}], "-", 
      RowBox[{"Sign", "[", 
       RowBox[{"t", "-", ".3"}], "]"}], "-", 
      RowBox[{"Sign", "[", 
       RowBox[{".72", "-", "t"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"n", "=", "1024"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"v", "=", 
     RowBox[{"g", "[", 
      RowBox[{"Range", "[", 
       RowBox[{"0", ",", 
        RowBox[{"1", "-", 
         RowBox[{"1", "/", "n"}]}], ",", 
        RowBox[{"1", "/", "n"}]}], "]"}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"ListPlot", "[", 
    RowBox[{"v", ",", 
     RowBox[{"Ticks", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Range", "[", 
         RowBox[{"0", ",", "n", ",", 
          RowBox[{"n", "/", "4"}]}], "]"}], ",", 
