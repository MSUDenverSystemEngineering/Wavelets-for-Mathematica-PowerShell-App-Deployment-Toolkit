Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"g", "[", "t_", "]"}], ":=", 
   RowBox[{
    RowBox[{"4", 
     RowBox[{"Sin", "[", 
      RowBox[{"4", " ", "Pi", " ", "t"}], "]"}]}], "-", 
    RowBox[{"Sign", "[", 
